# misc
alias e='subl -w'
alias ee='subl'
alias hexedit='open -a "Hex Fiend"'
alias locate='mdfind -name'

# git
alias gl='git pull'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status'
alias grm="git status | grep deleted | awk '{print \$3}' | xargs git rm"

# openssl
alias sha256='openssl sha -sha256'
